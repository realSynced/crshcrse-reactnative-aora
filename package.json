{
  "name": "crashcourse",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write",
    "build": "... existing-stuff ... && npm run fix-dependencies"
  },
  "dependencies": {
    "expo": "^51.0.32",
    "expo-constants": "~16.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "^0.74.0",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.31.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^13.6.4",
    "@react-native-community/cli-platform-android": "^13.6.4",
    "@react-native-community/cli-platform-ios": "^13.6.4",
    "@react-native/metro-config": "^0.74.0",
    "@rnx-kit/align-deps": "^2.5.5"
  },
  "private": true,
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.74"
      ],
      "capabilities": [
        "core",
        "core-android",
        "core-ios",
        "react",
        "safe-area",
        "screens"
      ]
    }
  }
}
